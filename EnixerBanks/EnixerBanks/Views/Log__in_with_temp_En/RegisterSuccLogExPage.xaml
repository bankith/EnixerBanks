<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EnixerBanks.Log__in_with_temp_En.RegisterSuccLogExPage">
    <ContentPage.Content>
        <AbsoluteLayout>
            <StackLayout Spacing="40" AbsoluteLayout.LayoutFlags="All" AbsoluteLayout.LayoutBounds="0,0,1,1" Padding="0,100,0,0">



                <Image Source="iconCheck" HeightRequest="100" VerticalOptions="Center"/>
                <Label Text="Register successfully" FontSize="27"  Margin="0,0,0,0" HorizontalOptions="Center" VerticalOptions="Center"/>

                <Label Text="Please enter NEXT To setup PIN" FontSize="Small" HorizontalOptions="Center" HorizontalTextAlignment="Center" VerticalOptions="Center" Margin="40,0,40,5" TextColor="Gray" />



            </StackLayout>


            <Button CornerRadius="0" Text="NEXT" Clicked="next_btn" TextColor="White" BackgroundColor="#347555" AbsoluteLayout.LayoutBounds="0,1,1,50" AbsoluteLayout.LayoutFlags="PositionProportional,WidthProportional" ></Button>

        </AbsoluteLayout>
    </ContentPage.Content>
</ContentPage>